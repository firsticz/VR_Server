{
  "extends": "nss/node",
  "rules": {
    "no-console": 0,
    "import/no-unresolved": 0,
    "no-buffer-constructor": 0,
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id", 
          "_get", 
          "_last", 
          "_orderBy", 
          "_reverse", 
          "_set",
          "_sumBy"
        ]
      }
    ]
  }
}